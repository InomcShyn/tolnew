{
  "manifest_version": 3,
  "name": "Proxy Auth Helper",
  "version": "1.0.0",
  "description": "Auto-fill proxy authentication",
  "permissions": [
    "webRequest",
    "webRequestAuthProvider"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  }
}